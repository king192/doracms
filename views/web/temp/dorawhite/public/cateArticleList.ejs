<!--分类文章模块-->
<div class="newItemList">
    <h3>分类文章</h3>
    <ul>
        <%if(cateArtileListData.length>0){%>
            <%cateArtileListData.forEach(function(item){%>
            <li>
                <a href="/details/<%=item._id%>.html"><%=item.title%></a>
            </li>
            <%})%>
        <%}else{%>
            <li class="text-center">暂无内容...</li>
        <%}%>

    </ul>
</div>